<!DOCTYPE html>
<html class="no-js" lang="{{ locale }}" dir="ltr">

{%- include "partials/head.njk" -%}

<body vocab="http://schema.org/" typeof="WebPage">

	{%- include "partials/skipnav.njk" -%}

{%- set otherLang = "fr" if locale == "en" else "en" -%}
{%- set otherLanguage = "Fran√ßais" if locale == "en" else "English" -%}

<header>
	<div id="wb-bnr" class="container">
		<div class="row">

			{%- include "partials/lang.njk" -%}

			<div class="brand col-xs-9 col-sm-5 col-md-4" property="publisher" typeof="GovernmentOrganization">
				<a href="{{ header[locale].governmentOfCanadaURL }}" property="url">
					<img src="https://www.canada.ca/etc/designs/canada//wet-boew/assets/sig-blk-{{ locale }}.svg" alt="{{ header[locale].governmentOfCanada }}" property="logo"/>
					<span class="wb-inv"> / <span lang="{{ otherLang }}">{{ header[locale].governmentOfCanada }}</span></span>
				</a>
				<meta property="name" content="{{ header[locale].governmentOfCanada }}">
				<meta property="areaServed" typeof="Country" content="Canada"/>
				<link property="logo" href="https://www.canada.ca/etc/designs/canada//wet-boew/assets/wmms-blk.svg"/>
			</div>

			<section id="wb-srch" class="col-lg-offset-4 col-md-offset-4 col-sm-offset-2 col-xs-12 col-sm-5 col-md-4">
				<h2>{{ header[locale].search }}</h2>
				<form action="https://canada.ca/{{ locale }}/sr/srb.html" method="get" name="cse-search-box" role="search">
					<div class="form-group wb-srch-qry">
						<label for="wb-srch-q" class="wb-inv">{{ header[locale].searchCanadaCa }}</label>
						<input id="wb-srch-q" list="wb-srch-q-ac" class="wb-srch-q form-control" name="q" type="search" value="" size="34" maxlength="170" placeholder="{{ header[locale].searchCanadaCa }}"/>
						<datalist id="wb-srch-q-ac"></datalist>
					</div>
					<div class="form-group submit">
						<button type="submit" id="wb-srch-sub" class="btn btn-primary btn-small" name="wb-srch-sub">
							<span class="glyphicon-search glyphicon"></span>
							<span class="wb-inv">{{ header[locale].search }}</span>
						</button>
					</div>
				</form>
			</section>

		</div>
	</div>

<div class="siteBanner">
	<div class="siteName container">
		{{ metadata.title[locale] | safe }}
		<small>{{ metadata.group[locale] | safe }}</small>
	</div>
</div>

</header>

	<main property="mainContentOfPage" resource="#wb-main" typeof="WebPageElement" {%- if tags -%} class="{{ tags }}"{%- endif -%}>

			<div class="container">
				<h1 property="name" id="wb-cont">{{ title | safe }}{%- if tags.length -%} <small>{{ media[locale][tags[0]] }}</small>{%- endif -%}</h1>

				{%- if tagline -%}
					<p class="tagline">{{ tagline | safe }}</p>
				{%- endif -%}

				{%- if description and showDescription != false -%}
					<div class="description">
						{{ description | markdownify | safe }}
					</div>
				{%- endif -%}

				{%- include "partials/alerts.njk" -%}

				{% block content %}
					{{ content | safe }}
				{% endblock %}
			</div>

		<section class="pagedetails container">
			<h2 class="wb-inv">{{ pagedetails[locale].pageDetailsHeading }}</h2>
			<div class="row">
				<div class="col-sm-6 col-md-5 col-lg-4">
					<dl id="wb-dtmd">
						<dt>{{ pagedetails[locale].dateModified }}</dt>
						<dd><time property="dateModified" datetime="{{ page.date | postDate }}">{{ page.date | postDate }}</time></dd>
					</dl>
				</div>
			</div>
		</section>

	</main>


<footer id="wb-info">
	<h2 class="wb-inv">{{ footer[locale].aboutThisSite }}</h2>
	<div class="gc-main-footer">
		<div class="container">
			<nav aria-labelledby="governmentOfCanadanHeading">
				<h3 id="governmentOfCanadanHeading">{{ footer[locale].governmentOfCanadan }}</h3>
				<ul class="list-col-xs-1 list-col-sm-2 list-col-md-3"><li><a href="{{ footer[locale].allContactsLink }}">{{ footer[locale].allContacts }}</a></li>
					<li><a href="{{ footer[locale].departmentsAndAgenciesURL }}">{{ footer[locale].departmentsAndAgencies }}</a></li>
					<li><a href="{{ footer[locale].aboutGovernmentURL }}">{{ footer[locale].aboutGovernment }}</a></li></ul>
				<h4><span class="wb-inv">{{ footer[locale].themesAndTopics }}</span></h4>
				<ul class="list-unstyled colcount-sm-2 colcount-md-3"><li><a href="{{ footer[locale].jobsURL }}">{{ footer[locale].jobs }}</a></li>
					<li><a href="{{ footer[locale].immigrationURL }}">{{ footer[locale].immigration }}</a></li>
					<li><a href="{{ footer[locale].travelURL }}">{{ footer[locale].travel }}</a></li>
					<li><a href="{{ footer[locale].businessURL }}">{{ footer[locale].business }}</a></li>
					<li><a href="{{ footer[locale].benefitsURL }}">{{ footer[locale].benefits }}</a></li>
					<li><a href="{{ footer[locale].healthURL }}">{{ footer[locale].health }}</a></li>
					<li><a href="{{ footer[locale].taxesURL }}">{{ footer[locale].taxes }}</a></li>
					<li><a href="{{ footer[locale].environmentURL }}">{{ footer[locale].environment }}</a></li>
					<li><a href="{{ footer[locale].defenceURL }}">{{ footer[locale].defence }}</a></li>
					<li><a href="{{ footer[locale].cultureURL }}">{{ footer[locale].culture }}</a></li>
					<li><a href="{{ footer[locale].policingURL }}">{{ footer[locale].policing }}</a></li>
					<li><a href="{{ footer[locale].transportURL }}">{{ footer[locale].transport }}</a></li>
					<li><a href="{{ footer[locale].internationalURL }}">{{ footer[locale].international }}</a></li>
					<li><a href="{{ footer[locale].financeURL }}">{{ footer[locale].finance }}</a></li>
					<li><a href="{{ footer[locale].scienceURL }}">{{ footer[locale].science }}</a></li>
					<li><a href="{{ footer[locale].indigenousURL }}">{{ footer[locale].indigenous }}</a></li>
					<li><a href="{{ footer[locale].veteransURL }}">{{ footer[locale].veterans }}</a></li>
					<li><a href="{{ footer[locale].youthURL }}">{{ footer[locale].youth }}</a></li>
				</ul>
			</nav>
		</div>
	</div>
	<div class="gc-sub-footer">
		<div class="container d-flex align-items-center">
			<nav aria-labelledby="governmentCorporateHeading">
				<h3 id="governmentCorporateHeading" class="wb-inv">{{ footer[locale].governmentCorporateHeading }}</h3>
				<ul>
					<li><a href="{{ footer[locale].socialURL }}">{{ footer[locale].social }}</a></li>
					<li><a href="{{ footer[locale].mobileURL }}">{{ footer[locale].mobile }}</a></li>
					<li><a href="{{ footer[locale].aboutURL }}">{{ footer[locale].about }}</a></li>
					<li><a href="{{ footer[locale].termsURL }}">{{ footer[locale].terms }}</a></li>
					<li><a href="{{ footer[locale].privacyURL }}">{{ footer[locale].privacy }}</a></li>
				</ul>
			</nav>
			<div class="wtrmrk align-self-end">
				<img src="https://wet-boew.github.io/themes-dist/GCWeb/GCWeb/assets/wmms-blk.svg" alt="Symbol of the Government of Canada" />
			</div>
		</div>
	</div>
</footer>

	<script src="https://ajax.googleapis.com/ajax/libs/jquery/2.2.4/jquery.js"></script>
	<script src="https://www.canada.ca/etc/designs/canada/wet-boew/js/wet-boew.min.js"></script>
	<script src="https://www.canada.ca/etc/designs/canada/wet-boew/js/theme.min.js"></script>
	<script src="/js/campaign-menu.js"></script>
	<script src="/js/lang-toggle-query.js"></script>
	{%- if customScript -%}
		<script src="{{ customScript }}"></script>
	{%- endif -%}
</body>
</html>
